{
  "name": "ridego-passenger",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "db:start": "npx supabase start",
    "db:stop": "npx supabase stop",
    "db:reset": "npx supabase db reset",
    "db:migrate": "npx supabase migration up --local",
    "db:status": "npx supabase status",
    "db:studio": "open http://localhost:54323",
    "dev": "npx supabase start && python3 -m http.server 3001",
    "serve": "python3 -m http.server 3001",
    "serve:alt": "python3 -m http.server 3001",
    "functions:serve": "npx supabase functions serve",
    "build": "npx eleventy",
    "dev:eleventy": "npx eleventy --serve"
  },
  "devDependencies": {
    "@11ty/eleventy": "^3.1.2",
    "pg": "^8.18.0",
    "supabase": "^2.15.8"
  },
  "dependencies": {
    "is-extendable": "^1.0.1"
  }
}
